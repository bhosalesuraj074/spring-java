<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "*-//Hibernate/Hibernate Mapping OTD 5.3//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.hibernate.Message" table="Message">
        
        <id name="id" >
        <generator class="increment"/>
        </id>
        <property name="message" column="MESSAGE-TEXT"/>
    </class>
</hibernate-mapping>
