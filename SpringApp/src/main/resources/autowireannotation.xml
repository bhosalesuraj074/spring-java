<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:annotation-config/>
    <bean id ="personInfo" class="com.springcore.annotation.PersonInfo">

    </bean>

    <bean id="employee" class="com.springcore.annotation.Employee">
        <property name="id" value="101" />
        <property name="name" value="Suraj" />
    </bean>
    <bean id="student" class="com.springcore.annotation.Student" autowire="byName">
        <property name="studentId" value="1" />
        <property name="name" value="Suraj" />
    </bean>


</beans>